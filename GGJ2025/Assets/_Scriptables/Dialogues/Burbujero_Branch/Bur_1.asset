    emotion: 0
    typeSpeed: 0.05
    dialogueTime: 1.5
    decisionTime: 10
    branches:
    - emotion: 1
      branchLines:
      - isPlayerLine: 0
        dialogueID: "\xA1Claro que no!, todos aman a mi esposa, es muy callada y
