        emotion: 0
        typeSpeed: 0.05
        dialogueTime: 1
        decisionTime: 0
        branches: []
      - isPlayerLine: 0
        dialogueID: "No lo s\xE9, cada vez que le pregunto me ignora. \xBFPero qu\xE9
