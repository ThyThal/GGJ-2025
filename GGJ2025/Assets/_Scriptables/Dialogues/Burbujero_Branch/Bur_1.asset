        isDecisionLine: 0
        emotion: 0
        typeSpeed: 0.05
        dialogueTime: 1
        decisionTime: 0
        branches: []
    - emotion: 5
      branchLines:
      - isPlayerLine: 0
        dialogueID: "A m\xED s\xED, pero a mi esposa nadie. Aunque\u2026"
        isDecisionLine: 0
        emotion: 0
        typeSpeed: 0.04
        dialogueTime: 1
        decisionTime: 0
        branches: []
      - isPlayerLine: 0
        dialogueID: "Ultimamente mi esposa estaba algo celosa de la Lic. Arrozal\xEDa.
