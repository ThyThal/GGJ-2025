        isDecisionLine: 0
        emotion: 5
        typeSpeed: 0.04
        dialogueTime: 1.5
        decisionTime: 0
        branches: []
      - isPlayerLine: 1
        dialogueID: "\xBFY qu\xE9 asuntos tiene su esposa con el conserje para pasar
