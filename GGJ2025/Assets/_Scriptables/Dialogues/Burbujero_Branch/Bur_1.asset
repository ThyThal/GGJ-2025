    isDecisionLine: 0
    emotion: 0
    typeSpeed: 0.05
    dialogueTime: 1
    decisionTime: 0
    branches: []
  - isPlayerLine: 1
    dialogueID: "\xBFHay alguien en esta f\xE1brica que odie o quiera hacerle da\xF1o
